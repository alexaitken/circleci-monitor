{
  "manifest_version": 2,

  "name": "CicleCI Monitor",
  "description": "Monitor your branches' build status on CircleCI",
  "version": "1.3.1",
  "browser_action": {
    "default_icon": {
      "19": "images/starting-19.png",
      "38": "images/starting-38.png"
    },
    "default_popup": "popup.html",
    "default_title": "CicleCI Monitor",
  },
  "background": {
    "scripts": [
      "vendor/zepto.js",
      "vendor/underscore.js",
      "vendor/backbone.js",
      "models.js",
      "app.js"
    ]
  },
  "permissions": [
    "https://circleci.com/*"
  ]

}
